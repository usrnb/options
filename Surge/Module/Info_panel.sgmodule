#!name=信息面板
#!desc=Info Panel

[Script]
SurgePro_ReloadProfile = type=generic,timeout=10,script-path=https://github.com/usrnb/options/raw/main/Surge/JS/surgepro_reloadprofile.js,argument=icon=crown.fill&color=#f6c970
TrafficStatistics = type=generic,timeout=10,script-path=https://github.com/usrnb/options/raw/main/Surge/JS/trafficstatistics.js,argument=icon=arrow.up.arrow.down.circle&color=#5d84f8
Netflix Unlock Detection = type=generic,script-path=https://github.com/usrnb/options/raw/main/Surge/JS/netflix_unlock_detection.js,argument=title=Netflix Unlock Detection
YouTube Premium Unlock Detection = type=generic,timeout=5,script-path=https://github.com/usrnb/options/raw/main/Surge/JS/youtube_premium_unlock_detection.js,argument=title=YouTube Premium Unlock Detection
Config Reload=script-path=https://github.com/usrnb/options/raw/main/Surge/JS/config_reload.js,type=generic
flushDNS = type=generic,timeout=5,script-path=https://raw.githubusercontent.com/usrnb/options/main/Surge/JS/flushDNS.js,argument=title=DNS Flush&icon=arrow.clockwise&color=#3d3d5b

[Panel]
SurgePro_ReloadProfile = script-name=SurgePro_ReloadProfile, title="Startup of Time", content="Please refresh", update-interval=1
TrafficStatistics = script-name=TrafficStatistics,title=Network traffic Statistics,content=Please refresh,update-interval=1
Netflix Unlock Detection = script-name=Netflix Unlock Detection,title=Netflix Unlock Detection,content=Please refresh.,update-interval=7200
YouTube Premium Unlock Detection = script-name=YouTube Premium Unlock Detection,title=YouTube Premium Unlock Detection,update-interval=7200
Config Reload = title=Config Reload,content=Config Reload,style=info,script-name=Config Reload,update-interval=-1
flushDNS = script-name=flushDNS, title="Flush DNS", content="Refresh", update-interval=600
